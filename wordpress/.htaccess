Options -Indexes +ExecCGI
AddHandler cgi-script .cgi .pl

# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /tle/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /ihanoi/index.php [L]
</IfModule>

# END WordPress
